        "build:staging": "npm run clean && env-cmd -e STAGING next build",
        "build:aws": "npm run clean && env-cmd -e AWS next build && npm run export",
        "buildSinExport:aws": "npm run clean && env-cmd -e AWS next build",
        "export": "next export -o outdir",
        "start": "next start",
        "clean": "npx rimraf .next",
        "lint": "npx eslint . --fix",
        "killPort": "npx kill-port 3012"
